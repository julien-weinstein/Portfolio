    "# YOUR CODE HERE\n",
    "#raise NotImplementedError()\n",
    "from sklearn.tree import DecisionTreeRegressor\n",
    "from sklearn.experimental import enable_iterative_imputer\n",
    "from sklearn.impute import IterativeImputer\n",
    "def answer_imputation_c():\n",
    "    result = None\n",
    "    CV_SPLITS = 5\n",
    "   \n",
    "    regressor = LogisticRegression(random_state = 42)\n",
    "\n",
    "    \n",
    "    imputer_2 = IterativeImputer(estimator = DecisionTreeRegressor(random_state = 42), missing_values=np.nan, add_indicator=True)\n",
    "    estimator_2 = make_pipeline(imputer_2, regressor)\n",
    "    impute_scores_2 = cross_val_score(estimator_2, X_missing, y_missing,\n",
    "                                    cv=CV_SPLITS)\n",
    "    print((np.mean(impute_scores_2)))\n",
    "    return (np.mean(impute_scores_2))\n",
    "    # YOUR CODE HERE\n",
    "    #raise NotImplementedError()\n",
    "  #  return result"
   ]
