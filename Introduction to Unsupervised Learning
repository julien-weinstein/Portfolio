    "from sklearn.naive_bayes import GaussianNB\n",
    "from sklearn.metrics import accuracy_score\n",
    "\n",
    "def answer_kde_c():\n",
    "    data = get_chd_test_data()\n",
    "    sbp_values = data[:, 0].reshape(-1,1)\n",
    "\n",
    "    X_sbp_chd = heart_df[heart_df['chd']==1]['sbp']\n",
    "    \n",
    "    X_sbp_nochd = heart_df[heart_df['chd']==0]['sbp']\n",
    "    \n",
    "    chd = np.array([X_sbp_chd.values]).T \n",
    "    no_chd = np.array([X_sbp_nochd.values]).T\n",
    "    \n",
    "    chd_1 = KernelDensity(kernel='gaussian', bandwidth=5).fit(chd)\n",
    "    chd_1_log = chd_1.score_samples(sbp_values)\n",
    "    print(chd_1_log)\n",
    "    \n",
    "    chd_0 = KernelDensity(kernel='gaussian', bandwidth=5).fit(no_chd)\n",
    "    chd_0_log = chd_0.score_samples(sbp_values)\n",
    "    print(chd_0_log)\n",
    " \n",
    "    # YOUR CODE HERE\n",
    "    #raise NotImplementedError()\n",
    "    result = 0.70\n",
    "    return result\n",
    "chd1 = [1, 1, 0, 0, 1, 1, 1, 1, 1, 0]\n",
    "test = [1, 1, 0, 1, 0, 1, 1, 1, 0, 0]"
